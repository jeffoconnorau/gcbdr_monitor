      "type": "stat",
      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${datasource}"
          },
          "query": "native = from(bucket: \"gcbdr\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"gcbdr_log_event\")\n  |> filter(fn: (r) => r[\"_field\"] == \"duration\")\n  |> filter(fn: (r) => r[\"type\"] != \"Restore\")\n\nmgmt = from(bucket: \"gcbdr\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"mgmt_console_job\")\n  |> filter(fn: (r) => r[\"_field\"] == \"duration\")\n  |> filter(fn: (r) => r[\"type\"] != \"restore\")\n\nunion(tables: [native, mgmt])\n  |> group(columns: [])\n  |> window(every: 1d)\n  |> count()\n  |> group()\n  |> mean()",
